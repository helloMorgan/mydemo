header {
    height: 2.2rem;
    width: 100%;
    color: #666;
}

header .inner {
    height: 2.2rem;
    overflow: hidden;
}

.bottom_nav ul li:nth-of-type(2) span{
    /*color:linear-gradient(red, blue);*/
    color: green;
    background-image: -webkit-linear-gradient(#00ad65, #01b6da);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}

.tra-travel{
    color: #41c8da;
}

body {
    /*height: 40rem;*/
    background: url("../../bitmap/account/user_bg.jpg");
    background-repeat: repeat;
    background-size: 100% auto;
}

header nav {
    position: relative;
}

nav {
    display: block;
}

header nav li:nth-of-type(1) {
    width: 7.5rem;
    top: 0;
    left: 0;
    position: absolute;
    z-index: 9;
}

.travel_order .iconfont {
    font-weight: 100!important;
}

header nav li:nth-of-type(2) {
    font-size: 0.87rem;
    width: 100%;
    text-align: center;
    line-height: 2.2rem;
}

header nav li:nth-of-type(3) {
    width: 7.5rem;
    top: 0;
    right: 0;
    position: absolute;
    z-index: 9;
}

header nav li {
    float: left;
    height: 2.2rem;
    color: #222;
}

header li h1 {
    font-size: 1rem;
    font-weight: 600;
    line-height: 2.2rem;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin: 0;
    color: #222;
}

header nav li:nth-of-type(3) span {
    float: right;
}

header nav li:nth-of-type(3) span:first-of-type {
    width: 1.6rem;
}

header nav li:nth-of-type(3) i.shopCart {
    display: inline-block;
    height: 1.6rem;
    width: 1.6rem;
    background: url(../../bitmap/account/my_set.png) no-repeat top left;
    background-size: 1.6rem, 1.75rem;
    float: right;
    margin-right: 0.75rem;
    margin-top: 0.25rem;
    margin-bottom: 0.25rem;
}

article {
    padding-bottom: 2.425rem;
}

article {
    width: 100%;
    height: 100%;
    margin: 0 auto;
    overflow-x: hidden;
    overflow-y: scroll;
}
