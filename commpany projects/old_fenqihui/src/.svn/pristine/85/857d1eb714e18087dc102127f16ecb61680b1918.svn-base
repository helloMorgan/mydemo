/*订单tab*/

.travel_order {
    width: 92%;
    margin: 0 auto;
    height: 3rem;
    display: flex;
    margin-bottom: 1.1rem;
    /*background: -webkit-linear-gradient(left, #4ecce0, #4dc699);*/
    /*!* Safari 5.1 - 6.0 *!*/
    /*background: -o-linear-gradient(right, #4ecce0, #4dc699);*/
    /*!* Opera 11.1 - 12.0 *!*/
    /*background: -moz-linear-gradient(right, #4ecce0, #4dc699);*/
    /*!* Firefox 3.6 - 15 *!*/
    /*background: linear-gradient(to right, #4ecce0, #4dc699);*/
    background: rgba(78,201,200,.5 );
    /* 标准的语法 */
}

.travel_order>div {
    flex: 1;
    text-align: center;
    font-size: 0.6rem;
    /*background: #4ecbd0;*/
    color: white;
    height: 3rem;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    justify-content: center;
    font-weight: 600;
}

.travel_order>div>div>p {
    margin-top: 0.2rem;
}

.travel_order>div>div>div {
    font-size: 1rem;
}

.travel_order .travel_order_active {
    background: white;
    border-radius: 0.2rem;
    color: #00B09A;
}

.travel_order .travel_order_active p {
    color: #606666;
}


/*订单详情*/

.travel_myOrder_info {
    /*padding: 0.9rem 0 0.6rem 0;*/
    background: white;
    margin: 1rem 0;
    border-radius: 0.2rem;
    box-shadow: 0 10px 20px gray!important          ;
}


/*共用 travel_myOrder_top travel_myOrder_mid */
.travel_myOrder_info .travel_myOrder_top {
    padding: 0.9rem 0.5rem 0 0.5rem;
}

.travel_myOrder_info .travel_myOrder_top .travel_myOrder_top_title {
    min-height: 1.1rem;
    line-height: 1.1rem;
    font-size: 0.8rem;
    color: #000;
    /*font-weight: 600;*/
    margin-bottom: 0.25rem;
}

.travel_myOrder_info .travel_myOrder_top .travel_myOrder_top_second {
    min-height: 1.2rem;
    font-size: 0.6rem;
    line-height: 0.8rem;
    color: #95adb2;
    margin-bottom: 0.2rem;
}

.travel_myOrder_info .travel_myOrder_mid .travel_myOrder_mid_left p {
    height: 1.2rem;
    line-height: 1.2rem;
    font-size: 0.65rem;
    display: flex;
}
.travel_myOrder_info .travel_myOrder_mid .travel_myOrder_mid_left .cmoneyIcon{
    font-size:0.6rem;
}
.travel_myOrder_info .travel_myOrder_mid .travel_myOrder_mid_left p .ticketNum{
    font-size:0.6rem;
}

.travel_myOrder_info .travel_myOrder_mid .travel_myOrder_mid_left .travel_myOrder_mid_class {
    color: #152323;
}
#my_bill .travel_month_order{
    display: inline-block;
    width:4.5rem;
    display: flex;
    justify-content: space-between;
    margin-right: 1rem;
}
#my_bill .travel_month_order>label{
    flex: 1;
    text-align: center;
}
#my_bill .travel_month_order>label:first-child{
    text-align: left;
}
#my_bill .travel_month_order>label:last-child{
    text-align: right;
}

.travel_myOrder_mid_left {
    padding-top: 0.9rem;
}
.travel_myOrder_info .travel_myOrder_mid .travel_myOrder_mid_left .travel_myOrder_mid_info {
    color: #293F46;
    display: inline-block;
    width:8rem;
    white-space:nowrap; overflow:hidden; text-overflow:ellipsis;
}
.travel_order_waiting .travel_myOrder_info .travel_myOrder_mid .travel_myOrder_mid_left .travel_myOrder_mid_info{
    display: inline-block;
    width:auto;
}

.travel_myOrder_info .travel_myOrder_mid {
    height: 6rem;
    line-height: 6rem;
    box-sizing: border-box;
    padding: 0 0.5rem 0.7rem 0.5rem;
    position: relative;
    overflow: hidden;
}

.travel_myOrder_info .travel_myOrder_mid .travel_myOrder_mid_right {
    margin: 0.3rem 0.5rem 0.3rem 0;
    position: absolute;
    top: 0;
    right: 0;
    width: 4.3rem;
    height: 3.9rem;
    padding-top: .5rem;
}

.travel_myOrder_info .travel_myOrder_mid .travel_myOrder_mid_right_success {
    position: absolute;
    top: 2rem;
    right: 0;
    width: 4.1rem;
    height: 4.1rem;
    background: url(../../bitmap/account/succes_trav.jpg) no-repeat;
    background-size: 4.3rem 3.9rem;
}

.travel_myOrder_info .travel_myOrder_mid .travel_myOrder_mid_right_fail {
    background: url(../../bitmap/account/fail_trav.jpg) no-repeat;
}

.travel_myOrder_info .travel_myOrder_mid .travel_myOrder_mid_right img {
    width: 100%;
    height: 100%;
    border-radius: 5px;
}


/*已出行图片样式框*/

.travel_myorder_mid_right2 {
    width: 4.5rem;
    height: 4.5rem;
    line-height: 3rem;
    text-align: center;
    border-radius: 50%;
    border: 0.2rem solid #86898a;
    position: absolute;
    bottom: -2rem;
    right: -0.4rem;
    font-size: 1rem;
    color: #86898a;
    transform: rotate(-30deg);
}

.travel_myOrder_info .travel_myOrder_mid .travel_myOrder_error {
    padding-top: 0.7rem;
    color: #ea6c5d;
    font-size: 0.6rem;
}

.travel_myOrder_info .travel_myOrder_bot {
    height: 3.25rem;
    line-height: 3.25rem;
    border-top: 1px solid #ccc;
    padding: 0 0.75rem;
}

.travel_myOrder_info .travel_myOrder_bot_center {
    height: 3.25rem;
    width: 100%;
    border-top: 1px solid #ccc;
    /*padding: 0 0.5rem;*/
    text-align: center;
}

.travel_myOrder_info .travel_myOrder_bot .travel_myOrder_bot_state {
    display: inline-block;
    line-height: 2.55rem;
    font-size: 0.8rem;
    color: #ea6c5d;
    font-weight: 500;
}

.travel_myOrder_info .travel_myOrder_bot .travel_myOrder_bot_state_calcel {
    display: inline-block;
    line-height: 2.55rem;
    font-size: 0.8rem;
    color: #00b0df;
    font-weight: 300;
}


