.travel-card-list1 {
    height: 1.5rem;
    line-height: 1.5rem;
    font-size: 0.6rem;
    text-align: center;
    color: #fff;
}

body {
    background: #f5f5f5;
}

.travel-card-samebg {
    /*background: -webkit-linear-gradient(left, #30cfb6, #00ad66);*/
    /*background: -o-linear-gradient(right, #01b6dc, #00ad66);*/
    /*background: -moz-linear-gradient(right, #01b6dc, #00ad66);*/
    /*background: linear-gradient(to right, #01b6dc, #00ad66);*/

    background: #30cfb6;
}

.travel-card-samebg2 {
    background: -webkit-linear-gradient(top,#ffeb01,#fecc66);
    background: linear-gradient(to bottom,#ffeb01,#fecc66);
}

.travel-card-container {
    margin-top: 1.125rem;
    font-size: .9rem;
    color: #333333;
}

.travel-card-list3>input {
    /*background: #f0f5f8;*/
}

.travel-card-list3>label {
    float: left;
}

.travel-card-list2,
.travel-card-list3 {
    height: 3rem;
    line-height: 3rem;
    display: flex;
    margin: 0 .75rem;
    background: #FFFFFF;

}

.travel-card-container > div input {

    flex: 1;
    text-align: left;
    font-size: 0.9rem;
    font-weight: 600;
    /*border: none;*/
    outline: none;
    /*height: 2.75rem;*/
}

.travel-card-container > div label {
    width: 3.5rem;
}

.travel-card-list4 {
    margin-top: 3rem;
    height: 2.25rem;
    line-height: 2.25rem;
    padding: 0 0.75rem;
    margin-left: 3rem;
}

.travel-card-list4 > a {
    display: block;
    width: 12rem;
    height: 100%;
    font-size: 0.7rem;
    font-weight: 600;
    line-height: inherit;
    color: #a55f5a;
    text-align: center;
    -webkit-border-radius: 1.25rem;
    -moz-border-radius: 1.25rem;
    border-radius: 1.25rem;
}

input:-moz-placeholder {
    color: #b8c8cb;
}

input::-webkit-input-placeholder {
    color: #b8c8cb;
}

input:-ms-input-placeholder {
    color: #b8c8cb;
}

input::-moz-placeholder {
    color: #b8c8cb;
}

.code_input {
}

::-webkit-input-placeholder{font-size:.65rem;}
::-moz-placeholder{font-size:.65rem;}
:-ms-input-placeholder{font-size:.65rem;}

input:-moz-placeholder{font-size:.65rem;}

.travel-card-list2>.iconfont {
     color: #eeeeee;
     font-size: .3rem;
     margin-left: .75rem;
     margin-right: .4rem;
 }


.travel-card-list3>.iconfont {
    color: #eeeeee;
    font-size: .3rem;
    margin-left: .75rem;
    margin-right: .4rem;
}

.travel-card-list3 {
    margin-top: .5rem;
}

