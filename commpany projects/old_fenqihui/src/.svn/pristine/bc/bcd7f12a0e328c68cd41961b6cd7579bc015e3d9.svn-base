	border: 2px solid #ffe113;
	border-radius: 1.2rem;
	text-align: center;
	margin-bottom: 1.5rem;
	
}
*/
.itemDetalis{
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	height: 2.25rem;
	margin-bottom: 1.5rem;
	margin-top: .5rem;
}
.itemTab{
	width: 7.25rem;
	display: flex;
	justify-content: center;
	align-items: center;
	font-size: 0.9rem;
	color: #ffc549;
	border: 2px solid #ffe113;
	border-radius: 1.2rem;

}
.goPay{
	height: 2.25rem;
	width: 7.25rem;
	display: flex;
	justify-content: center;
	line-height: 2.25rem;
	align-self: center;
	font-size: 0.9rem;
	color: #ffc549;
	border: 2px solid #ffe113;
	border-radius: 1.2rem;
	margin-bottom: 1.5rem;
	margin: 0 auto;
	
}
.forwardPay{
	display: flex;
	align-self: center;
	font-size: 0.8rem;
	color: #555555;
}



.title_star>.iconfont {
	float: left;
}
.green{
	color:#00ceb7;
}
.gray{
	
	color: #999;
}
.overdueFee{
	position: absolute;
	top: 1.2rem;
	right: 0;
	color: #ea3200;
}

.item_pay {
	display: inline-block;
	width: 5rem;
	height: 2rem;
	border: 2px solid #ffe113;
	border-radius: 1.2rem;
	text-align: center;
	line-height: 2rem;
	color: #ff4500;
	float: right;
	margin-top: .4rem;
}

.dataListItemTop {
	margin-top: 1rem;
}





