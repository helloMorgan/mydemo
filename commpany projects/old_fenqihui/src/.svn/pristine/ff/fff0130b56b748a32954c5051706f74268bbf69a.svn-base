body,html{
	background: #f4f4f4;
	width: 100%;
	height: 100%;
}
header{
    height: 6.25rem;
    /*background: url("http://fenqihuiyou.oss-cn-qingdao.aliyuncs.com/product/1492669084574_p7-精品推荐.jpg") no-repeat;*/
    background-size: 100% 100%;
}
#mainOrder{
	margin: 0.75rem auto;
	height: 7.8rem;
	width: 17.25rem;
	background: #FFFFFF;
	display: flex;
	display: -webkit-flex;
	flex-direction: column;
	border-radius:0.2rem ;
	
}
.mainOrder_title{
	padding:0rem 1rem;
	width: 15.25rem;
	display: flex;
	background: #00ceb7;
	flex: 1;
	align-items: center;
	justify-content: flex-start;
	font-size: 0.75rem;
	color: #FFFFFF;
    display: -webkit-box;
	overflow: hidden;
	white-space: nowrap;
	text-overflow:ellipsis;
	border-radius:0.2rem 0.2rem 0 0 ;
}
.mainOrder_info{
	padding:0rem 1rem;
	display: flex;
	flex: 4;
	flex-direction: column;
	justify-content: space-around;
	font-size: 0.65rem;
	color: #cdcdcd;
	
	
}
.mainOrder_info div{
	
	display: flex;
	flex: 1;
	justify-content: space-between;
	align-items: center;
	position: relative;
}
.mainOrder_info span{
	display: flex;
	flex: 1;
	
}
.mainOrder_info  i{
	color: black;
	text-align: center;
	display: flex;
	flex: 1;
}
.mainOrder_info a{
	display: flex;
	flex: 1;
}
.select{
	position: relative;
	left: 1.5rem;
}
.submit_notice{
    width: 0.6rem;
    height: 0.6rem;
    background: url("../../bitmap/travel/submit_notice.gif");
    background-size: 100% 100%;
    position: absolute;
    top: 1rem;
    left: 14.3rem;
}

footer{
	width: 100%;
	height: 3rem;
	background: -webkit-linear-gradient(right,#ffeb01,#fecc66);
    background: linear-gradient(to right,#ffeb01,#fecc66);
    color: #a55e5b;
    font-size: 0.9rem;
    text-align: center;
    line-height: 3rem;
	position: fixed;
	z-index: 999999;
	bottom: 0;
}
#mainDetails{
	margin: 0 auto;
	margin-top: 0.75rem ;
	width: 17.25rem;
	background: #FFFFFF;
	border-radius:0.2rem 0.2rem 0 0 ;
	
}
.mainDetails_tab{
	height: 2.5rem;
	display: flex;
	flex-direction: row;
	font-size: 0.75rem;
	border-bottom: 1px solid #CDCDCD;

	
}
.mainDetails_tab span{
	display: flex;
	flex: 1;
	justify-content:space-around;
	line-height:2.5rem ;
	height: 2.5rem;
}
.mainDetails_text{
	padding:0rem 1rem;
	width: 15.25rem;
	padding-bottom:2rem;
}


.stagesOrderBorder:last-child{
	border: none;
}
.mainDetails_title{
	height: 2rem;
	line-height: 2rem;


}
.mainDetails_img{
	background: url("../../bitmap/travel/myDetailsBanner.gif") no-repeat;
    background-size: 100% 100%;
}

.mainDetails_article a{
	font-size: 0.75rem;
	font-weight: 600;
	height: 2rem;
	line-height: 2rem;
	
}
.mainDetails_article p{
	padding-bottom: 0.75rem;
}
#popup{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: black;
	opacity: 0.5;
	z-index: 99;
}
.stagesOrder{
	width: 14.75rem;
	background: #FFF;
	position: absolute;
	top: 5rem;left: 0.75rem;
	margin: 0 auto;
	display: flex;
	display: -webkit-flex;
	flex-direction:column;
	justify-content:space-around;
	z-index: 100;
	padding: 1.25rem;
	border-radius: 0.3rem;
	font-size:0.75rem ;
	color: #777;
	
}
 .stagesOrder i{
	color:#e6381a ;
	font-weight: 800;
}
.stagesOrder a{
	color: #000000;
	font-weight: 800;
}

.slide{
	display: flex;
}
.stagesOrderBorder{
	height: 2.5rem;
	line-height: 2.5rem;
	border-bottom:1px solid #D8D8D8 ;
	
}
.stages_interest{
	background: #ffc12a;
    font-size: 0.7rem;
    color: #ffffff;
    width:2rem ;
    height: 1.2rem;
    border-radius: 0.4rem;
    line-height: 1.2rem;
    text-align: center;
    position: absolute;
	top: 7.3rem;right: 4rem;
}
.mainDetails_chooseOne{
	border-bottom:1px solid red;
	color: red;
}
.mainDetails_chooseTwo{
	border-bottom:1px solid #D8D8D8;
}
#text img{
	    width: 15.25rem;
	    position: relative;
	  	margin: 0;padding: 0; 
	
}
#mainDetails_title img{
	width:15.25rem;
	
}


